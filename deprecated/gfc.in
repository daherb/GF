#!/bin/sh

prefix="@prefix@"

case "@host@" in
      *-cygwin)
          prefix=`cygpath -w "$prefix"`;;
esac

exec_prefix="@exec_prefix@"
GF_BIN_DIR="@bindir@"
GF_DATA_DIR="@datadir@/GF-@PACKAGE_VERSION@"

GFBIN="$GF_BIN_DIR/gf"

if [ ! -x "${GFBIN}" ]; then
   GF_BIN_DIR=`dirname $0`
   GFBIN="$GF_BIN_DIR/gf"
fi

if [ ! -x "${GFBIN}" ]; then
   GFBIN=`which gf`
fi

if [ ! -x "${GFBIN}" ]; then
    echo "gf not found."
    exit 1
fi

exec $GFBIN --batch "$@"
